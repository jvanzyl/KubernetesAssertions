#!/usr/bin/env groovy
@Library('github.com/stakater/fabric8-pipeline-library@master')

mavenNode(mavenImage: 'openjdk:8') {
    container(name: 'maven') {
        stage('Checkout') {
            checkout scm
        }

        stage('Canary Release') {
            mavenCanaryRelease2 {
                version = '1.0'
            }
        }
    }
}